{
  "name": "mbartmann-s3cmd",
  "version": "1.0.0",
  "author": "mbartmann",
  "summary": "Install S3cmd - Command line tool for managing Amazon S3 and CloudFront services",
  "license": "MIT",
  "source": "https://github.com/mbartmann/puppet-s3cmd",
  "project_page": "https://github.com/mbartmann/puppet-s3cmd",
  "issues_url": "https://github.com/mbartmann/puppet-s3cmd/issues",
  "dependencies": [
